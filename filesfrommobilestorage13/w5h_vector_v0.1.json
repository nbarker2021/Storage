{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "urn:schemas:w5h-vector:v0.1",
  "title": "W5H Vector v0.1",
  "type": "object",
  "required": [
    "contexts"
  ],
  "properties": {
    "contexts": {
      "type": "array",
      "minItems": 5,
      "items": {
        "type": "object",
        "required": [
          "name",
          "score",
          "evidence"
        ],
        "properties": {
          "name": {
            "type": "string"
          },
          "score": {
            "type": "number",
            "minimum": 0,
            "maximum": 1
          },
          "evidence": {
            "type": "string"
          }
        }
      }
    }
  },
  "additionalProperties": false
}